{
  "name": "@tombatossals/chords-db",
  "version": "0.5.1",
  "description": "Instrument Chords Database",
  "main": "index.js",
  "scripts": {
    "test": "jest src",
    "stdout": "babel-node src/generate",
    "build": "babel-node --max-old-space-size=4096 src/generate json",
    "import": "babel-node src/import",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prettier": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tombatossals/chords-db.git"
  },
  "keywords": [
    "music",
    "node",
    "javascript"
  ],
  "author": {
    "name": "David Rubert",
    "email": "david.rubert@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tombatossals/chords-db/issues"
  },
  "homepage": "https://github.com/tombatossals/chords-db#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-plugin-wildcard": "^5.0.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  },
  "jest": {
    "verbose": false,
    "bail": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
